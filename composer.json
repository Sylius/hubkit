{
    "name": "sylius/hub-kit",
    "description": "HubKit allows project(s) maintainers to easily manage there GitHub repositories. Modified version of park-manager/hub-kit.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": "^8.0",
        "php-http/guzzle6-adapter": "^2.0.2",
        "ddd/slug": "~1.0.0",
        "guzzlehttp/psr7": "^1.9.0",
        "knplabs/github-api": "v3.0.0",
        "pimple/pimple": "^3.5.0",
        "rollerworks/version": "^0.3.1",
        "symfony/console": "^3.4",
        "symfony/event-dispatcher": "~3.4",
        "symfony/filesystem": "~3.4",
        "symfony/process": "~3.4",
        "webmozart/console": "dev-master@dev",
        "slim/psr7": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.21",
        "symfony/debug": "^3.4",
        "symfony/flex": "^2.2",
        "symfony/phpunit-bridge": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "HubKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HubKit\\Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
